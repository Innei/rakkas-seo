{
  "name": "rakkas-seo",
  "version": "0.6.0-4",
  "description": "rakkas SEO is a plug in that makes managing your SEO easier in rakkas.js projects.",
  "author": {
    "name": "Gary Meehan",
    "email": "garymeehan@outlook.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "esm",
    "readme.md",
    "tsconfig.json",
    "types",
    "src"
  ],
  "engines": {
    "pnpm": ">=7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "bump": {
    "before": [
      "npm run build"
    ],
    "publish": true
  },
  "scripts": {
    "prebuild": "rm -rf lib && rm -rf esm",
    "build": "tsc --build ./src/tsconfig.build.json && tsc --build src/tsconfig.cjs.json",
    "postbuild": "tsc-alias -p src/tsconfig.build.json && tsc-alias -p src/tsconfig.cjs.json && npm run types",
    "types": "rm -rf types && tsc --build src/tsconfig.types.json && tsc-alias -p src/tsconfig.types.json",
    "test": "vitest",
    "dev": "vite"
  },
  "peerDependencies": {
    "rakkasjs": "0.6.0"
  },
  "devDependencies": {
    "@innei/eslint-config-react-ts": "0.9.4",
    "@innei/eslint-config-ts": "0.9.4",
    "@innei/prettier": "0.9.4",
    "@types/node": "18.0.6",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rakkasjs": "0.6.0",
    "react": "^18.2.0",
    "react-dom": "18",
    "tsc-alias": "1.7.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.2",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.18.1"
  }
}